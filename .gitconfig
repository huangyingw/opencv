[remote "opencv"]
    url = https://github.com/opencv/opencv.git
    fetch = +refs/heads/*:refs/remotes/opencv/*
[remote "origin"]
    url = git@github.com:huangyingw/opencv.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[push]
    remote = origin
[gsync]
    remote = opencv
    branch = master
[merge]
    allow = true
[commit]
    message = n
[branch "master"]
    remote = origin
    merge = refs/heads/master
[branch "dev"]
    remote = origin
    merge = refs/heads/dev
